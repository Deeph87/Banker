{% extends 'base.html.twig' %}

{% block title %}Sign up!{% endblock %}

{% block body %}
    {{ form_start(form) }}
    {{ form_row(form.email) }}
    {{ form_row(form.password.first) }}
    {{ form_row(form.password.second) }}
    <button class="btn btn-lg btn-primary" type="submit">Register!</button>
    {{ form_end(form) }}
{% endblock %}